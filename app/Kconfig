menu "Zephyr"
source "Kconfig.zephyr"
endmenu

menu "Application Configuration"

config NASAL_TEMP_SAMPLE_PERIOD
	int "Nasal temperature sample period in microseconds (us)"
	default 10000
	help
	  Configure the nasal temperature sample period in microseconds.
	  Default is 10000us.

endmenu

module = APP
module-str = APP
source "subsys/logging/Kconfig.template.log_config"

config DISPLAY_LOGO
	bool "Enable display logo"
	default y
	depends on DISPLAY
	help
	  Enable inclusion of the logo in the display output.
	  When disabled, the logo source files will not be compiled
	  and the logo will not be displayed.
